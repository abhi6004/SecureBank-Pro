@{
    foreach (var user in Model)
    {
        <div>
            <p>Name: @user.full_name</p>
            <p>Email: @user.email</p>
            <p>Region: @user.assigned_region</p>
            <p>Can Export: @(user.can_export_reports == "true" ? "Yes" : "No")</p>
            <p>Last Login: @user.last_login_time</p>

            <button onclick="EdituserForm('@user.email')">Edit</button>
            <button onclick="DeletedUser()">Deleted</button>
            <input id="Upload-id-Auditor" type="hidden" value="@user.id" />
            <button id="Upload">Upload</button>
        </div>
    }
}

<script src="~/js/Dashboard/Auditor.js"></script>

